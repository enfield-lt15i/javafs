
public class CastingHard {

	public static void main(String[] args) {
		
		double a = 1.1;
		
		double b = 1;
		double b2 = (double) 1; 
		
		System.out.println(b); //1.0
		
//		int c = 1.1; //undefined
		//solution
		double d = 1.1; //Change type of 'd' to 'double'
		
		int e = (int) 1.1; //Add cast to 'int' (강제)
		
		System.out.println(e); //0.1 손실

	}

}
