public class DataType{
	public static void main(String[] args) {
		
		System.out.println(6); //Number
		System.out.println("six"); //String
		System.out.println("6"); //String 6
		
		System.out.println(6+6); //12
		System.out.println("6"+"6"); //66
		
		System.out.println(6*6); //36
//		System.out.println("6"*"6"); //undefined
		
		System.out.println("1111".length()); //4
		System.out.println("Hello World!".length()); //12
		System.out.println("안녕 세상".length()); //5
		System.out.println(1111); //1111
//		System.out.println(1111.length()); //undefined
		System.out.println("Lucky 7".length()); //7
		
		
		
		System.out.println('H'); //Character 문자
	}
}